            "tsConfig": "projects/ui-kendo/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/ui-kendo/**/*.ts",
              "projects/ui-kendo/**/*.html"
            ]
          }
        }
      }
    },
    "ui-material": {
      "projectType": "library",
      "root": "projects/ui-material",
      "sourceRoot": "projects/ui-material/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/ui-material/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/ui-material/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/ui-material/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/ui-material/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/ui-material/**/*.ts",
              "projects/ui-material/**/*.html"
            ]
          }
        }
      }
    },
    "ui-basic": {
      "projectType": "library",
      "root": "projects/ui-basic",
      "sourceRoot": "projects/ui-basic/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/ui-basic/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/ui-basic/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/ui-basic/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/ui-basic/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/ui-basic/**/*.ts",
              "projects/ui-basic/**/*.html"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "schematicCollections": [
      "@angular-eslint/schematics"
    ]
  }
}
